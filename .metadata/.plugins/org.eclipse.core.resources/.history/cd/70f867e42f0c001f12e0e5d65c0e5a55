package oops_task;

public class grades {

	
	String name;
	int grade;
	
	
	void print()
	{
		
		 if (grade >= 90) {
	            System.out.println(name + " has an A grade.");
	        } else if (grade >= 80) {
	            System.out.println(name + " has a B grade.");
	        } else if (grade >= 70) {
	            System.out.println(name + " has a C grade.");
	        } else if (grade >= 60) {
	            System.out.println(name + " has a D grade.");
	        } else {
	            System.out.println(name + " has an F grade.");
	        }
	    }
}
