package java_task_loops;

public class Reversed_loop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		 System.out.println("Reverse Counting from 10 to 1:");
		  //  Print numbers from 10 to 1 in reverse
		 
	        int count = 10;
	        
	        while (count >= 1) 
	        {
	            System.out.print(count + " ");
	            
	            count--;
	        }
	}

}
